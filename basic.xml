<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html>
<html xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>
    <head>

        <b:include data='blog' name='all-head-content' />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />

        <title>
            <b:if cond='data:blog.pageType == &quot;index&quot;'>
                <data:blog.pageTitle />
                <b:else />
                <b:if cond='data:blog.pageType != &quot;error_page&quot;'>
                    <data:blog.pageName />
                    |
                    <data:blog.title />
                    <b:else />
                    Page Not Found |
                    <data:blog.title />
                </b:if>
            </b:if>
        </title>

        <b:skin>
            <![CDATA[
     
        body {
            background: white;
            color: black;
            font-family: Arial, Inter, sans-serif;
            font-size: 17px;
        }

        @media screen and (max-width: 1000px) {
            body {
                font-size: 16px;
            }
        }        

        html, body, main, article {
            overflow-x: hidden;
        }

        .post pre code {
            /*width: 600px;*/
            width: 100%;
            overflow-x: auto;
            font-family: 'Cascadia Code','Droid Sans Mono', monospace;
            padding: 12px;
            border-radius: 5px;
            display: block;
            background-color: #1b1c1d;
            color: #adadad;
        }

        hr {
            border: none;
            border-top: 1px solid black;
        }

        a {
            color: royalblue;
            text-decoration: none;
        }

        img {
            max-width: 100%;
            height: auto;
        }

        .nav {
            margin: 0;
            padding: 0;
        }
        .nav li {
            display: inline;
            margin-right: 5px;
        }

        .nav li:not(:last-child)::after {
            content: " ~ ";
        }
        
        .blog-posts {
            margin-top:15px;
        }

        .blog-feeds {
            margin-top: 30px;
        }

        a:hover {
            text-decoration: underline;
        }

        .post-title a, .title a {
            text-decoration: none;
            color: inherit;
        }

        .post-title {
            margin-top:3px;
        }

        .post {
            margin-top: 15px;
        }

        @media only screen and (min-width: 750px) {
            body {
                width: 75%;
                margin: auto;
            }
        }
        @media only screen and (min-width: 1300px) {
            body {
                width: 50%;
                margin: auto;
            }
        }

        @media only screen and (min-width: 1600px) {
            body {
                width: 40%;
                margin: auto;
            }
        }

        @media only screen and (max-width: 750px) {
            .post pre code {
                max-width: 100%;
            }
        }

        html {
            box-sizing: border-box;
        }
        *, *:before, *:after {
            box-sizing: inherit;
        }

        .feed-link {
            padding-right: 10px;
        }

        body {
            margin-top: 30px;
            margin-bottom: 20px;
        }

        .email-icon * {
            fill: #000000;
        }

        @media (prefers-color-scheme: dark) {
            body, html, main {
                background-color: #121212;
                color: white;
            }
            hr {
                border-top-color: gray;
            }
            a {
                color: royalblue;
            }
            .post pre code {
                background-color: #151718;
            }
            .email-icon * {
                fill: #ffffff;
            }
        }
	
  ]]>
        </b:skin>

    </head>
    <body>
        <header class="pageheader">
            <b:section class='header' id='header' showaddelement='yes'>
                <b:widget id='Header1' locked='true' type='Header'>
                    <b:widget-settings>
                        <b:widget-setting name='displayUrl' />
                        <b:widget-setting name='displayHeight'>0</b:widget-setting>
                        <b:widget-setting name='sectionWidth'>-1</b:widget-setting>
                        <b:widget-setting name='shrinkToFit'>false</b:widget-setting>
                        <b:widget-setting name='imagePlacement'>BEHIND</b:widget-setting>
                        <b:widget-setting name='displayWidth'>0</b:widget-setting>
                    </b:widget-settings>
                    <b:includable id='main'>
                        <div id='header-inner'>
                            <div class='titlewrapper'>
                                <h1 class='title'>
                                    <b:tag cond='data:blog.url != data:blog.homepageUrl' expr:href='data:blog.homepageUrl' name='a'>
                                        <data:title />
                                    </b:tag>
                                </h1>
                            </div>
                            <div class='descriptionwrapper'>
                                <p class='description'>
                                    <span>
                                        <data:description />
                                    </span>
                                </p>
                            </div>
                        </div>
                    </b:includable>
                </b:widget>
                <b:widget id='PageList1' locked='false' title='' type='PageList'>
                    <b:widget-settings>
                        <b:widget-setting name='pageListJson'>
                            <![CDATA[{'link0': {'href': 'http://webdevelopment-en-meer.blogspot.com/', 'title': 'Start', 'position': 0}, '9098950833866459076': {'href': 'https://webdevelopment-en-meer.blogspot.com/p/over-mij.html', 'title': 'Over mij', 'position': 1}, 'link1': {'href': 'https://geheimesite.nl', 'title': 'geheimesite.nl', 'position': 2}}]]>
                        </b:widget-setting>
                        <b:widget-setting name='homeTitle'>Start</b:widget-setting>
                    </b:widget-settings>
                    <b:includable id='main'>
                        <div class='widget-content'>
                            <b:if cond='data:mobile'>
                                <select expr:id='data:widget.instanceId + &quot;_select&quot;'>
                                    <b:loop values='data:links' var='link'>
                                        <option expr:value='data:link.href'>
                                            <b:attr cond='data:link.isCurrentPage' name='selected' value='selected' />
                                            <data:link.title />
                                        </option>
                                    </b:loop>
                                </select>
                                <span class='pagelist-arrow'>&amp;#9660;</span>
                                <b:else />
                                <ul class="nav">
                                    <b:loop values='data:links' var='link'>
                                        <li>
                                            <b:class cond='data:link.isCurrentPage' name='selected' />
                                            <a expr:href='data:link.href'>
                                                <data:link.title />
                                            </a>
                                        </li>
                                    </b:loop>
                                </ul>
                            </b:if>
                        </div>
                        <hr />
                    </b:includable>
                </b:widget>
            </b:section>
        </header>
        <main>
            <b:section class='main' preferred='yes' id='main' showaddelement='yes'>
                <b:widget id='Blog1' locked='true' title='Blog Posts' type='Blog' version='1'>
                    <b:widget-settings>
                        <b:widget-setting name='showDateHeader'>false</b:widget-setting>
                        <b:widget-setting name='showShareButtons'>false</b:widget-setting>
                        <b:widget-setting name='showCommentLink'>false</b:widget-setting>
                        <b:widget-setting name='showAuthor'>false</b:widget-setting>
                        <b:widget-setting name='disableGooglePlusShare'>true</b:widget-setting>
                        <b:widget-setting name='reactionsLabel' />
                        <b:widget-setting name='showAuthorProfile'>false</b:widget-setting>
                        <b:widget-setting name='showLabels'>true</b:widget-setting>
                        <b:widget-setting name='showLocation'>false</b:widget-setting>
                        <b:widget-setting name='postLabelsLabel'>Tags:</b:widget-setting>
                        <b:widget-setting name='showTimestamp'>false</b:widget-setting>
                        <b:widget-setting name='showBacklinks'>false</b:widget-setting>
                        <b:widget-setting name='showInlineAds'>false</b:widget-setting>
                        <b:widget-setting name='showReactions'>false</b:widget-setting>
                    </b:widget-settings>
                    <b:includable id='main' var='top'>
                        <!-- Desktop view -->
                        <b:if cond='!data:mobile'>

                            <div class='blog-posts hfeed'>

                                <b:include data='top' name='status-message' />

                                <b:loop values='data:posts' var='post'>

                                    <b:if cond='data:post.isDateStart and not data:post.isFirstPost'>
                                        &lt;/div&gt;&lt;/div&gt;
                                    </b:if>
                                    <b:if cond='data:post.isDateStart'>
                                        &lt;div class=&quot;date-outer&quot;&gt;
                                    </b:if>
                                    <b:if cond='data:post.dateHeader'>
                                        <h2 class='date-header'>
                                            <span>
                                                <data:post.dateHeader />
                                            </span>
                                        </h2>
                                    </b:if>
                                    <b:if cond='data:post.isDateStart'>
                                        &lt;div class=&quot;date-posts&quot;&gt;
                                    </b:if>
                                    <div class='post-outer'>
                                        <b:include data='post' name='post' />
                                        <b:include cond='data:blog.pageType in {&quot;static_page&quot;,&quot;item&quot;}' data='post' name='comment_picker' />
                                    </div>

                                    <!--<b:if cond='data:numPosts > 1'>-->
                                    <hr />
                                    <!--</b:if>-->

                                </b:loop>
                                <b:if cond='data:numPosts != 0'>
                                &lt;/div&gt;&lt;/div&gt;
                                </b:if>
                            </div>

                            <!-- navigation -->
                            <b:include name='nextprev'/>

                            <!-- Feed links -->
                            <b:include name='feedLinks' />

                            <b:else />
                            <!-- Mobile view -->
                            <b:include name='mobile-main' />
                        </b:if>
                    </b:includable>
                    <b:includable id='post' var='post'>

                        <b:if cond='data:view.isHomepage or data:view.search'>
                            <article class="post">
                                <data:post.timestamp/>
                                <h2 class="post-title entry-title">
                                    <a expr:href='data:post.link ? data:post.link : data:post.url'>
                                        <data:post.title />
                                    </a>
                                </h2>

                                <p><b:eval expr='data:post.longSnippet snippet { length: 600, links: false, linebreaks: true }'/></p>
                            </article>
                        <b:else />
                            <article class='post hentry uncustomized-post-template' itemprop='blogPost' itemscope='itemscope' itemtype='http://schema.org/BlogPosting'>

                                <b:if cond='data:post.firstImageUrl'>
                                    <meta expr:content='data:post.firstImageUrl' itemprop='image_url' />
                                </b:if>
                                <meta expr:content='data:blog.blogId' itemprop='blogId' />
                                <meta expr:content='data:post.id' itemprop='postId' />

                                <a expr:name='data:post.id' />
                                <b:if cond='data:post.title'>
                                    <h2 class='post-title entry-title' itemprop='name'>
                                        <b:if cond='data:post.link or (data:post.url and data:blog.url != data:post.url)'>
                                            <a expr:href='data:post.link ? data:post.link : data:post.url'>
                                                <data:post.title />
                                            </a>
                                            <b:else />
                                            <data:post.title />
                                        </b:if>
                                    </h2>
                                </b:if>

                                <header class='post-header'>
                                    <div class='post-header-line-1' />
                                </header>

                                <div class='post-body entry-content' expr:id='&quot;post-body-&quot; + data:post.id' expr:itemprop='(data:blog.metaDescription ? &quot;&quot; : &quot;description &quot;) + &quot;articleBody&quot;'>
                                    <data:post.body />
                                    <div style='clear: both;' /> <!-- Clear for photos floats -->
                                </div>

                                <b:if cond='data:post.hasJumpLink'>
                                    <div class='jump-link'>
                                        <a expr:href='data:post.url + &quot;#more&quot;' expr:title='data:post.title'>
                                            <data:post.jumpText />
                                        </a>
                                    </div>
                                </b:if>

                                <footer class='post-footer'>
                                    <div class='post-footer-line post-footer-line-1'>

                                        <span class='post-timestamp'>
                                            <b:if cond='data:top.showTimestamp'>
                                                <data:top.timestampLabel />
                                                <b:if cond='data:post.url'>
                                                    <meta expr:content='data:post.url.canonical' itemprop='url' />
                                                    <a class='timestamp-link' expr:href='data:post.url' rel='bookmark' title='permanent link'>
                                                        <abbr class='published' expr:title='data:post.timestampISO8601' itemprop='datePublished'>
                                                            <data:post.timestamp />
                                                        </abbr>
                                                    </a>
                                                </b:if>
                                            </b:if>
                                        </span>

                                        <span class='post-comment-link'>
                                            <b:include cond='data:blog.pageType not in {&quot;item&quot;,&quot;static_page&quot;}                          and data:post.allowComments' data='post' name='comment_count_picker' />
                                        </span>

                                    </div>

                                    <div class='post-footer-line post-footer-line-2'>
                                        <span class='post-labels'>
                                            <b:if cond='data:top.showPostLabels and data:post.labels'>
                                                <data:postLabelsLabel />
                                                <b:loop values='data:post.labels' var='label'>
                                                    <a expr:href='data:label.url' rel='tag'><data:label.name /></a><b:if cond='not data:label.isLast'>,</b:if>
                                                </b:loop>
                                            </b:if>
                                        </span>
                                    </div>

                                </footer>
                            </article>
                        </b:if>
                    </b:includable>
                    <b:includable id='nextprev'>
                        <div class='blog-pager' id='blog-pager'>
                            <b:if cond='data:newerPageUrl'>
                            <span id='blog-pager-newer-link'>
                                <a class='blog-pager-newer-link' expr:href='data:newerPageUrl' expr:id='data:widget.instanceId + &quot;_blog-pager-newer-link&quot;' expr:title='data:newerPageTitle'>
                                &#8592; Terug
                                </a>
                            </span>
                            </b:if>

                            <b:if cond='data:olderPageUrl'>
                            <span id='blog-pager-older-link'>
                                <a class='blog-pager-older-link' expr:href='data:olderPageUrl' expr:id='data:widget.instanceId + &quot;_blog-pager-older-link&quot;' expr:title='data:olderPageTitle'>
                                Laad meer &#8594;
                                </a>
                            </span>
                            </b:if>

                            <a class='home-link' expr:href='data:blog.homepageUrl'>
                            <data:homeMsg/>
                            </a>

                        </div>
                        <div class='clear'/>
                    </b:includable>
                    <b:includable id='feedLinksBody' var='links'>
                        <div class='feed-links'>
                            <span style="padding-right:5px;">Volg me via RSS: </span>
                            <b:loop values='data:links' var='f'>
                                <a class='feed-link' expr:href='data:f.url' expr:type='data:f.mimeType' target='_blank'>
